{
  "id": "json-config-crowdstrike-dest",
  "type": "crowdstrike_next_gen_siem",
  "description": "CrowdStrike Next-Gen SIEM destination from JSON config",
  "url": "https://ingest.us-2.crowdstrike.com/api/ingest/hec/connection-id/v1/services/collector",
  "token": "your-crowdstrike-token",
  "authType": "manual",
  "compress": true,
  "concurrency": 5,
  "extraHttpHeaders": [],
  "failedRequestLoggingMode": "none",
  "flushPeriodSec": 1,
  "format": "JSON",
  "maxPayloadEvents": 0,
  "maxPayloadSizeKb": 4096,
  "onBackpressure": "block",
  "pqCompress": "none",
  "pqMaxFileSize": "1 MB",
  "pqMaxSize": "5GB",
  "pqMode": "error",
  "pqOnBackpressure": "block",
  "pqPath": "$CRIBL_HOME/state/queues",
  "rejectUnauthorized": true,
  "responseHonorRetryAfterHeader": false,
  "responseRetrySettings": [],
  "safeHeaders": [],
  "systemFields": ["cribl_pipe"],
  "streamtags": ["crowdstrike", "json-config"],
  "timeoutRetrySettings": {
    "timeoutRetry": false
  },
  "timeoutSec": 30,
  "useRoundRobinDns": false
}
