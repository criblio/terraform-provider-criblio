{
  "id": "json-config-cribl-http-dest",
  "type": "cribl_http",
  "description": "Cribl HTTP destination from JSON config",
  "compression": "gzip",
  "concurrency": 5,
  "dnsResolvePeriodSec": 600,
  "excludeSelf": false,
  "extraHttpHeaders": [],
  "failedRequestLoggingMode": "none",
  "flushPeriodSec": 1,
  "loadBalanceStatsPeriodSec": 300,
  "loadBalanced": false,
  "maxPayloadEvents": 0,
  "maxPayloadSizeKb": 4096,
  "onBackpressure": "block",
  "pqCompress": "none",
  "pqMaxFileSize": "1 MB",
  "pqMaxSize": "5GB",
  "pqMode": "error",
  "pqOnBackpressure": "block",
  "pqPath": "$CRIBL_HOME/state/queues",
  "rejectUnauthorized": true,
  "responseHonorRetryAfterHeader": false,
  "responseRetrySettings": [],
  "safeHeaders": [],
  "systemFields": ["cribl_pipe"],
  "streamtags": ["cribl-http", "json-config"],
  "timeoutRetrySettings": {
    "timeoutRetry": false
  },
  "timeoutSec": 30,
  "url": "http://cribl-receiver:10080",
  "useRoundRobinDns": false
}
