{
  "id": "json-config-cribl-tcp-dest",
  "type": "cribl_tcp",
  "description": "Cribl TCP destination from JSON config",
  "compression": "gzip",
  "connectionTimeout": 10000,
  "dnsResolvePeriodSec": 600,
  "excludeSelf": false,
  "host": "cribl-receiver",
  "port": 10300,
  "loadBalanceStatsPeriodSec": 300,
  "loadBalanced": false,
  "logFailedRequests": false,
  "maxConcurrentSenders": 0,
  "onBackpressure": "block",
  "pqCompress": "none",
  "pqMaxFileSize": "1 MB",
  "pqMaxSize": "5GB",
  "pqMode": "error",
  "pqOnBackpressure": "block",
  "pqPath": "$CRIBL_HOME/state/queues",
  "systemFields": ["cribl_pipe"],
  "streamtags": ["cribl-tcp", "json-config"],
  "throttleRatePerSec": "0",
  "tls": {
    "disabled": true
  }
}
