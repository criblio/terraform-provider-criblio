{
  "id": "json-config-prometheus-dest",
  "type": "prometheus",
  "description": "Prometheus destination from JSON config",
  "url": "http://prometheus:9090/api/v1/write",
  "authType": "none",
  "concurrency": 5,
  "extraHttpHeaders": [],
  "failedRequestLoggingMode": "none",
  "flushPeriodSec": 10,
  "maxPayloadEvents": 1000,
  "maxPayloadSizeKb": 1024,
  "metricRenameExpr": "name.replace(/[^a-zA-Z0-9_]/g, '_')",
  "metricsFlushPeriodSec": 60,
  "onBackpressure": "queue",
  "pqCompress": "none",
  "pqMaxFileSize": "1 MB",
  "pqMaxSize": "5GB",
  "pqMode": "error",
  "pqOnBackpressure": "block",
  "pqPath": "$CRIBL_HOME/state/queues",
  "rejectUnauthorized": true,
  "responseHonorRetryAfterHeader": false,
  "responseRetrySettings": [],
  "safeHeaders": [],
  "systemFields": ["cribl_pipe"],
  "streamtags": ["prometheus", "json-config"],
  "timeoutRetrySettings": {
    "timeoutRetry": false
  },
  "timeoutSec": 30,
  "useRoundRobinDns": false
}
