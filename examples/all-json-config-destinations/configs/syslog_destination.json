{
  "id": "json-config-syslog-dest",
  "type": "syslog",
  "description": "Syslog destination from JSON config",
  "appName": "Cribl",
  "connectionTimeout": 10000,
  "facility": 16,
  "host": "syslog.example.com",
  "port": 12514,
  "protocol": "udp",
  "severity": 6,
  "loadBalanced": false,
  "logFailedRequests": false,
  "maxRecordSize": 1500,
  "messageFormat": "rfc3164",
  "octetCountFraming": false,
  "onBackpressure": "block",
  "pqCompress": "none",
  "pqMaxFileSize": "1 MB",
  "pqMaxSize": "5GB",
  "pqMode": "error",
  "pqOnBackpressure": "block",
  "pqPath": "$CRIBL_HOME/state/queues",
  "systemFields": ["cribl_pipe"],
  "streamtags": ["syslog", "json-config"],
  "throttleRatePerSec": "0",
  "timestampFormat": "syslog",
  "writeTimeout": 60000,
  "tls": {
    "disabled": true
  }
}
