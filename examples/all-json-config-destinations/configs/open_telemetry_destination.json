{
  "id": "json-config-otel-dest",
  "type": "open_telemetry",
  "description": "OpenTelemetry destination from JSON config",
  "endpoint": "http://otel-collector:4317",
  "authType": "none",
  "compress": "gzip",
  "concurrency": 5,
  "connectionTimeout": 10000,
  "extraHttpHeaders": [],
  "failedRequestLoggingMode": "none",
  "flushPeriodSec": 1,
  "httpCompress": "gzip",
  "keepAlive": true,
  "keepAliveTime": 30,
  "maxPayloadSizeKb": 4096,
  "metadata": [],
  "onBackpressure": "block",
  "otlpVersion": "1.3.1",
  "protocol": "http",
  "pqCompress": "none",
  "pqMaxFileSize": "1 MB",
  "pqMaxSize": "5GB",
  "pqMode": "error",
  "pqOnBackpressure": "block",
  "pqPath": "$CRIBL_HOME/state/queues",
  "rejectUnauthorized": true,
  "responseHonorRetryAfterHeader": false,
  "responseRetrySettings": [],
  "safeHeaders": [],
  "systemFields": ["cribl_pipe"],
  "streamtags": ["otel", "json-config"],
  "timeoutRetrySettings": {
    "timeoutRetry": false
  },
  "timeoutSec": 30,
  "useRoundRobinDns": false
}
